# phpstan.neon
parameters:
  level: 5
  paths:
    - app
    - routes
    - database
  bootstrapFiles:
    - vendor/autoload.php
  ignoreErrors:
    - '#Access to an undefined property Illuminate\\Contracts\\Auth\\Authenticatable::.*#'
    - '#Call to an undefined static method App\\Models\\.*#'
    - '#Call to an undefined method Illuminate\\Contracts\\Auth\\Factory::.*#'


